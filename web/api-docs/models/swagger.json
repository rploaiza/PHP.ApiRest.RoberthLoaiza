        "license": {
            "name": "MIT",
            "url": "http://localhost:8000/api-docs/LICENSE.txt"
        },
        "version": "0.1.0"
    },
    "host": "localhost:8000",
    "basePath": "/api/v1",
    "schemes": [
        "http",
        "https"
    ],
    "consumes": [
        "application/json",
