                    "403": {
                        "$ref": "#/responses/403_Forbidden_Response"
                    },
                    "404": {
                        "$ref": "#/responses/404_Resource_Not_Found_Response"
                    }
                },
                "security": [
                    {
                        "ResultsSecurity": []
                    }
                ]
            },
            "post": {
                "tags": [
